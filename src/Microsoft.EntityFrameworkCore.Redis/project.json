{
  "version": "1.1.0-*",
  "description": "Redis data store for Entity Framework (to be used for testing purposes).",
  "dependencies": {
    "Microsoft.EntityFrameworkCore": "1.1.0-*",
    "NETStandard.Library": "1.6.1-*",
    "StackExchange.Redis": "1.1.608"
  },
  "buildOptions": {
    "warningsAsErrors": true,
